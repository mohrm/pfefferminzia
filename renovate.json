{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "ignoreUnstable": false,
  "packageRules": [
  {
    "matchDatasources": ["docker"],  
    "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)(?<prerelease>-beta\\d+(?:\\.\\d+)?)?$"
  }
  ]
}
